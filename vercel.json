{
    "version": 2,
    "builds":[
        {
            "src":"server/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"15mb", "runtime":"python-3.10"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/python"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"server/wsgi.py"
        }
    ]
}